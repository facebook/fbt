{
  "collectCoverage": false,
  "globals": {
    "__DEV__": true
  },
  "setupFiles": [
    "<rootDir>/jest/setup.js"
  ],
  "modulePaths": [
    "src",
    "src/example",
    "../runtime/shared",
    "../runtime/shared/FbtNumber",
    "../runtime/nonfb",
    "../runtime/nonfb/mocks",
    "node_modules"
  ],
  "testRegex": "/__tests__/.+-test.js$",
  "testPathIgnorePatterns": [
    "/output/",
    "/node_modules/"
  ],
  "timers": "fake",
  "moduleNameMapper": {
    "\\.(css)$": "<rootDir>/jest/css.js"
  },
  "transform": {
    "\\.(bmp|gif|jpg|jpeg|png|psd|svg|webp|ttf|ktx|wav|ogg|mp4|webm)$":
      "<rootDir>/jest/assets.js",
    ".*": "<rootDir>/jest-transformer"
  }
}
